@model Xlns.BusBook.UI.Web.Models.DashBoardInfo
@{
    ViewBag.Title = "DashBoard";
}
@section head{

    <link href="@Url.Content("~/Content/tiledList.css")" rel="stylesheet" type="text/css" media="screen"/>
}
@section scripts{
    <script src="@Url.Content("~/Content/scripts/viaggio.lista.js")" type="text/javascript"></script>
}
<div id="content-wrapper" class="container_16">
    <!-- main -->
    <div id="main" class="grid_16">
        <div id="left-columns" class="grid_4">
            <div class="grid_4 alpha">
                <div class="sidemenu">
                    @{Html.RenderAction("ShowTileShort","Agenzia", new { agenziaVM = Model.DettaglioAgenzia} );}
                </div>
                <div class="sidemenu">
                    @{Html.RenderAction("ListPartial", "Flyer", new { idAgenzia = Model.DettaglioAgenzia.agenzia.Id});}
                </div>
            </div>
        </div>
        <div class="grid_4 omega">
            @{Html.RenderAction("ListPartial", "Viaggio");}
        </div>
        <!-- main ends -->
    </div>
    <!-- contents end here -->
</div>
