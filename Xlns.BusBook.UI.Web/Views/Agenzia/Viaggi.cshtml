@model Xlns.BusBook.UI.Web.Models.ViaggiAgenziaModel

@{
    ViewBag.Title = Model.TitoloRelazione + Model.AgenziaPartecipante.Nome;
}
@section head{
    <link href="@Url.Content("~/Content/tiledList.css")" rel="stylesheet" type="text/css" media="screen"/>
}
@section scripts{
    <script src="@Url.Content("~/Content/scripts/viaggio.lista.js")" type="text/javascript"></script>
}

<div id="content-wrapper">
    <h3>@ViewBag.Title</h3>
    <div id="main" >
     @foreach (var item in Model.Viaggi)
        {
            Html.RenderAction("ViaggioTiledDetail", "Viaggio", new { viaggio = item });
        }
    </div>
</div>